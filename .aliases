#aliases
alias ls='ls --group-directories-first --color=auto'
alias l='ls -lh'
alias l.='ls -lhd .*'
alias la='ls -lhA'
alias h='history'
alias j='journalctl'
alias cp='cp -iv'
alias mv='mv -iv'
alias rm='rm -iv'
alias df='df -h'
alias free='free -h'
alias frei='df; free'
alias vi='nvim'
alias mux='tmuxinator'
alias tar='tar -xvzf'
alias sys='sudo SYSTEMD_EDITOR=/bin/vi /usr/bin/systemctl'
